{
	"name": "@keenmate/svelte-adminlte",
	"version": "5.0.8",
	"repository": "https://github.com/keenmate/svelte-adminlte.git",
	"private": false,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"prepublishOnly": "npm run package",
		"preview": "vite preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"default": "./dist/actions/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"CBuffer": "^2.2.0",
		"litepicker": "^2.0.11",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.54.1",
		"svelte": "^5.0.0",
		"svelte-preprocess": "^6.0.0",
		"tippy.js": "^6.3.7",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"vite": "^5.4.4"
	},
	"devDependencies": {
		"@types/inputmask": "^5.0.3",
		"@types/toastr": "^2.1.43"
	},
	"peerDependencies": {
		"@keenmate/js-common-helpers": "^3.0.4",
		"@keenmate/svelte-common-helpers": "^3.1.2",
		"admin-lte": "=3.0.5",
		"bootstrap": "^4.4.1",
		"icheck-bootstrap": "^3.0.1",
		"inputmask": "^5.0.8",
		"jquery": "^3.7.0",
		"svelte-i18n": "^4.0.1"
	},
	"overrides": {
		"summernote": "file:./__I_REMOVED_THIS_PACKAGE_BECAUSE_OF_HUSKY_NOT_FOUND_ERROR_DURING_INSTALL_PHASE__"
	}
}
